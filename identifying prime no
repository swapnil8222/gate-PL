

#include <stdio.h>

int main()
{
    int i, n; 
    printf("enter a number: ");
    scanf("%d", &n);
    if(n==1)
    printf("1 is not a prime");
    for(i=2; i<n; i++)
    {
        if(n%i==0)
        printf("%d is not a prime", n);
    }
    if(i==n)
    printf("%d is prime", n);
    
    return 0;

}
