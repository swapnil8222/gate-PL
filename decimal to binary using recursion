// Online C compiler to run C program online
#include <stdio.h>
void f(int n )
{
    if(n==0||n==1){
        printf("%d", n);
        return;
    }
    else
    {
        f(n/2);
        printf("%d", n%2);
    }
}
int main() {
    // Write C code here
    int n;
    printf("enter a no to convert: ");
    scanf("%d", &n);
    f(n);
}
